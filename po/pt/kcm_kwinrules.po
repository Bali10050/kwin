#: optionsmodel.cpp:198
#, kde-format
msgid "Unimportant"
msgstr "Sem importância"

#: optionsmodel.cpp:199
#, kde-format
msgid "Exact Match"
msgstr "Correspondência Perfeita"

#: optionsmodel.cpp:200
